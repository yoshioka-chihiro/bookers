<h1>Books</h1>
<table>
  <!-- theadは表のヘッダーという意味 -->
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>
   <tbody>
     <tr>
       <td></td>
     </tr>
   </tbody>
</table>

<h2>New book</h2>
<%= form_with model: @book, url: '/books', method: :post do |f| %>

<h4>Title</h4>
<%= f.text_field:title %>

<h4>Body</h4>
<%= f.text_area:body %>

<%= f.submit 'Create Book' %>
<% end %>
